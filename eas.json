{
	"builds": {
		"android": {
			"release": {
				"workflow": "managed",
				"cache": { "key": "nocache", "disabled": true },
				"releaseChannel": "production",
				"env": {
					"ENVIRONMENT": "production"
				},
				"buildType": "app-bundle"
			},
			"development": {
				"workflow": "managed",
				"buildType": "development-client",
				"distribution": "internal",
				"cache": { "key": "nocache", "disabled": true }
			}
		},

		"ios": {
			"release": {
				"workflow": "managed",
				"releaseChannel": "production",
				"cache": {
					"key": "clear",
					"disabled": true,
					"cacheDefaultPaths": false
				},
				"env": {
					"ENVIRONMENT": "production"
				}
			},
			"development": {
				"workflow": "managed",
				"buildType": "development-client",
				"distribution": "internal",
				"releaseChannel": "staging",
				"cache": { "key": "nocache", "disabled": true }
			}
		}
	}
}
